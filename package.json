{
  "name": "@aldeia/audit-sdk",
  "version": "0.1.1",
  "private": false,
  "description": "SDK de audit logging para serviços internos (HTTP → serviço de auditoria)",
  "keywords": [
    "audit",
    "logging",
    "sdk",
    "typescript",
    "kafka",
    "microservices",
    "observability"
  ],
  "homepage": "https://github.com/mathauscm/audit-sdk#readme",
  "bugs": {
    "url": "https://github.com/mathauscm/audit-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mathauscm/audit-sdk.git"
  },
  "license": "MIT",
  "author": "mathausdev",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "test": "test"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "prepublishOnly": "npm run build",
    "format": "echo \"no formatter configured\"",
    "lint": "npm run typecheck",
    "test": "npm run build && node --test \"test/**/*.test.mjs\""
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
